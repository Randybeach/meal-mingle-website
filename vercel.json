{
  "functions": {
    "server.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/recipe/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/invite/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/cookbook/(.*)",
      "destination": "/server.js"
    }
  ]
}